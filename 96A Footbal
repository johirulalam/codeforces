#include<stdio.h>
#include<string.h>
 
int main()
{
    char ch[100];
    int num,i,a=0;
    scanf("%s",&ch);
    num = strlen(ch);
    for(i=0;i<num;i++)
    {
        if(ch[i]== '0' && ch[i+1]== '0' && ch[i+2]== '0' && ch[i+3]== '0' && ch[i+4]== '0' && ch[i+5]== '0' && ch[i+6]== '0' )
        {
            a=7;
            break;
 
        }
        else if(ch[i]== '1' && ch[i+1]== '1' && ch[i+2]== '1' && ch[i+3]== '1' && ch[i+4]== '1' && ch[i+5]== '1' && ch[i+6]== '1' )
        {
            a=7;
            break;
 
        }
 
        else{
            continue;
        }
    }
    if(a==7)
    {
            printf("YES");
    }
 
    else{
        printf("NO");
    }
 
 
    return 0;
 
}
